# ============================================
# EMPORIONPROS - API KEYS
# ============================================
# Copy this file to .env and fill in your keys
# In Netlify: Site Settings > Environment Variables
# ============================================

# FREE APIS (get these first)
# -------------------------------------------
# Census API: https://api.census.gov/data/key_signup.html
CENSUS_API_KEY=your_census_key_here

# Walk Score: https://www.walkscore.com/professional/api-sign-up.php
WALKSCORE_API_KEY=your_walkscore_key_here

# PAID APIS (add when ready)
# -------------------------------------------
# RentCast: https://app.rentcast.io/app/api (free 50 calls/mo, $130 for 2K)
RENTCAST_API_KEY=

# ATTOM Data: https://api.gateway.attomdata.com (starts $95/mo)
ATTOM_API_KEY=

# Zoneomics: https://zoneomics.com/api (starts $99/mo)
ZONEOMICS_API_KEY=

# AI (for Aria recommendations)
# -------------------------------------------
# Anthropic Claude: https://console.anthropic.com
ANTHROPIC_API_KEY=

# GEOCODING
# -------------------------------------------
# Google Maps: https://console.cloud.google.com (free $200/mo credit)
GOOGLE_MAPS_API_KEY=

# Optional: OpenCage (free 2,500/day) https://opencagedata.com
OPENCAGE_API_KEY=
